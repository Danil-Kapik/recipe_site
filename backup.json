elds": {"recipe": 6, "ingredient": 8, "quantity": "1.00"}}, {"model": "recipes.recipeingredient", "pk": 28, "fields": {"recipe": 7, "ingredient": 23, "quantity": "500.00"}}, {"model": "recipes.recipeingredient", "pk": 29, "fields": {"recipe": 7, "ingredient": 24, "quantity": "500.00"}}, {"model": "recipes.recipeingredient", "pk": 30, "fields": {"recipe": 7, "ingredient": 12, "quantity": "2.00"}}, {"model": "recipes.recipeingredient", "pk": 31, "fields": {"recipe": 7, "ingredient": 20, "quantity": "400.00"}}, {"model": "recipes.recipeingredient", "pk": 32, "fields": {"recipe": 7, "ingredient": 18, "quantity": "3.00"}}, {"model": "recipes.recipeingredient", "pk": 33, "fields": {"recipe": 7, "ingredient": 26, "quantity": "2.00"}}]